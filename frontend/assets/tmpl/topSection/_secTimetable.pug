                    .time 10:00<br>|<br>12:00
                    .time 10:00<br>|<br>12:00
                    .time 10:00<br>|<br>12:00
                    .time 10:00<br>|<br>12:00
                    .time 10:00<br>|<br>12:00
                .secTimetable_sections
                    each sessionInfo in speakers.findByRoom("901+902")
                        +session(sessionInfo)
                .secTimetable_sections
                    each sessionInfo in speakers.findByRoom("905")
                        +session(sessionInfo)
                .secTimetable_sections
