#secStaff
    .container
        .secStaff_contents
            h2.secStaff_heading
                span Staff
            .secStaff_staffs
                each staff in locals.staffs
                    .secStaff_staff
                        a(href=staff.url)
                            .secStaff_icon
                                 img(src="/assets/staffs/" + staff.img)
                            .secStaff_name
                                span #{staff.name}